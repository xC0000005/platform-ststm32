{
  "build": {
    "core": "stm32",
    "cpu": "cortex-m4",
    "extra_flags": "-DSTM32F40_41xxx -DSTM32F407xx",
    "f_cpu": "168000000L",
    "ldscript": "armstrap_eagle_512.ld",
    "mcu": "stm32f407vet6",
    "variant": "stm32f4"
  },
  "debug": {
    "default_tools": [
      "blackmagic"
    ],
    "onboard_tools": [
      "blackmagic"
    ],
    "openocd_target": "stm32f4x",
    "svd_path": "STM32F40x.svd"
  },
  "frameworks": [
    "cmsis",
    "spl",
    "stm32cube"
  ],
  "name": "Armstrap Eagle 512",
  "upload": {
    "maximum_ram_size": 196608,
    "maximum_size": 524288,
    "protocol": "blackmagic",
    "protocols": [
      "jlink",
      "stlink",
      "blackmagic"
    ]
  },
  "url": "http://docs.armstrap.org/en/latest/hardware-overview.html",
  "vendor": "Armstrap"
}
