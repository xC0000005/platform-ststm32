{
  "build": {
    "core": "stm32",
    "cpu": "cortex-m4",
    "extra_flags": "-DSTM32F40_41xxx -DSTM32F417xx",
    "f_cpu": "168000000L",
    "ldscript": "armstrap_eagle_1024.ld",
    "mcu": "stm32f417vgt6",
    "variant": "stm32f4"
  },
  "debug": {
    "default_tools": [
      "blackmagic"
    ],
    "onboard_tools": [
      "blackmagic"
    ],
    "openocd_target": "stm32f4x",
    "svd_path": "STM32F41x.svd"
  },
  "frameworks": [
    "cmsis",
    "spl",
    "stm32cube"
  ],
  "name": "Armstrap Eagle 1024",
  "upload": {
    "maximum_ram_size": 196608,
    "maximum_size": 1048576,
    "protocol": "blackmagic",
    "protocols": [
      "jlink",
      "stlink",
      "blackmagic"
    ]
  },
  "url": "http://docs.armstrap.org/en/latest/hardware-overview.html",
  "vendor": "Armstrap"
}
